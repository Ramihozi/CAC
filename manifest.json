{
    "name": "Type and Talk",
    "version": "1.0",
    "description": "You type it talks, for those who have limited ability to see what they type.",
	"icons": {
			"16": "favicon16x16.png",
			"128": "favicon128x128.png"
	}, 
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
	"content_scripts": [{
        "matches": ["<all_urls>"],
        "all_frames": true,
		"run_at": "document_end",
        "js":  ["content.js"]
    }],
	"permissions": [
		"activeTab"
	],
	"web_accessible_resources": [
		"Sound/LetterA.mp3",
		"Sound/LetterB.mp3",
		"Sound/LetterC.mp3",
		"Sound/LetterD.mp3",
		"Sound/LetterE.mp3",
		"Sound/LetterF.mp3",
		"Sound/LetterG.mp3",
		"Sound/LetterH.mp3",
		"Sound/LetterI.mp3",
		"Sound/LetterJ.mp3",
		"Sound/LetterK.mp3",
		"Sound/LetterL.mp3",
		"Sound/LetterM.mp3",
		"Sound/LetterN.mp3",
		"Sound/LetterO.mp3",
		"Sound/LetterP.mp3",
		"Sound/LetterQ.mp3",
		"Sound/LetterR.mp3",
		"Sound/LetterS.mp3",
		"Sound/LetterT.mp3",
		"Sound/LetterU.mp3",
		"Sound/LetterV.mp3",
		"Sound/LetterW.mp3",
		"Sound/LetterX.mp3",
		"Sound/LetterY.mp3",
		"Sound/LetterZ.mp3",
		"Sound/Number0.mp3",
		"Sound/Number1.mp3",
		"Sound/Number2.mp3",
		"Sound/Number3.mp3",
		"Sound/Number4.mp3",
		"Sound/Number5.mp3",
		"Sound/Number6.mp3",
		"Sound/Number7.mp3",
		"Sound/Number8.mp3",
		"Sound/Number9.mp3",
		"Sound/Periodkey.mp3",
		"Sound/Shiftkey.mp3",
		"Sound/Tabkey.mp3",
		"Sound/Deletekey.mp3"
		
	],
    "manifest_version": 2
  }
